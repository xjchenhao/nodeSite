mixin catalogueNode(catalogue,index,parentId)
    for item,i in catalogue
        if item.level == index && item.parent == parentId && catalogue[i].isShow
            li
                a(href="#{item.link}")
                    i(class=item.icon)
                    | &nbsp;&nbsp;#{item.title}
                    if catalogue[i + 1] && catalogue[i + 1].level > item.level
                        i.fa.arrow
                if catalogue[i + 1] && catalogue[i + 1].level > item.level
                    ul.nav.nav-second-level.collapse
                        +catalogueNode(catalogue,index + 1,item.id)

